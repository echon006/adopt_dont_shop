<fieldset>
<legend>Creating new level</legend>
<%= form_with url: '/applications', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :street_address %>
  <%= form.text_field :street_address %>
  <br>
  <%= form.label :city %>
  <%= form.number_field :city %>
  <br>
  <%= form.label :state %>
  <%= form.number_field :state%>
  <br>
  <%= form.label :zip_code %>
  <%= form.number_field :zip_code%>
  <br>
  <%= form.submit "Submit" %>

<% end %>
</fieldset>
